{
  "name": "backend-node",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "vitest run --config vitest.config.ts",
    "test:unit": "vitest run --config vitest.config.ts --exclude test/integration/** --exclude tests/integration/**",
    "test:integration": "vitest run --config vitest.config.ts test/integration tests/integration",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:typecheck": "tsc -p tsconfig.test.json --noEmit",
    "migrate": "tsx src/migrations/run-migrations.ts",
    "check:migrations": "tsx src/migrations/check-migrations.ts",
    "verify:task04": "tsx src/scripts/verify-task04.ts",
    "verify:task05": "tsx src/scripts/simulate-multi-requests.ts",
    "verify:task07": "tsx src/scripts/verify-task07.ts",
    "mint:test-jwt": "tsx src/scripts/mint-test-jwt.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "fastify": "^5.2.1",
    "openai": "^4.76.0",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.5"
  }
}
